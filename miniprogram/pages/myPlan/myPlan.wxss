/* pages/myPlan/myPlan.wxss */
.plan-main-box {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  padding: 20rpx;
}
.add-plan {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: blue;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 10px;
  left: 50%;
  z-index: 999;
  margin-left: -50rpx;
}
.loading-style {
  height: 60px;
}
.year-box {
  position: fixed;
  z-index: 999;
  width: 60px;
  background: #f5f5f5;
  height: 40px;
  font-size: 20px;
  line-height: 40px;
  text-align: center;
  left: 50%;
  margin-left: -30px;
  top: 0px;
  border-radius: 0 0px 10px 10px;

}
.nodata {
  width: 100%;
  padding-top: 100px;
  text-align: center;
  font-size: 12px;
  color:#666;
}